{
  "compilerOptions": {
      "module": "esnext",
      "target": "esnext",
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "allowJs": true,
      "checkJs": false,
      "sourceMap": true,
      "moduleResolution": "node",
      "isolatedModules": true,
      "esModuleInterop": true,
      "strict": true,
      "outDir": "_ui/skin/dist",
      "baseUrl": "./",
      "paths": {
        "@/*": ["_ui/skin/src/*"],
      }
  },
  "exclude": ["node_modules"],
  "include": ["_ui/skin/src"]
}